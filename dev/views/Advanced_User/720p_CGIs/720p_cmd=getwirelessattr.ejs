<% include ../../_partials/template/_head.ejs %>

<body>
    <% include ../../_partials/template/_outdated.ejs %>

    <div id="wrapper">

        <header><% include ../../_partials/template/_header.ejs %></header>
        <!-- Navbar-->

            <div class="container mb">
                <h1>720p CGI Commands - Get Wireless Attribute</h1>
                <hr>
            </div>

            <div class="clearfix"></div>

            <div class="container main">

              <!-- ######################################## Body ####################################### -->

              <section class="row" style="margin-top: 15px;">
                <!-- Media Container -->

                <div class="col-xs-12">

                  <h2>Network :: Get Wireless Attribute</h2>
                  <h3>param.cgi?cmd=getwirelessattr</h3>

                  <div class="table-responsive">
                    <table class="table table-hover">
                      <tr>
                        <th>Function</th>
                        <td>Get Wifi Parameter</td>
                      </tr>
                      <tr>
                        <th>Authority</th>
                        <td>admin</td>
                      </tr>
                      <tr>
                        <th>Syntax</th>
                        <td><code>/cgi-bin/hi3510/param.cgi[?cmd=getwirelessattr]</code></td>
                      </tr>
                      <tr>
                        <th rowspan="6">Return</th>
                        <td>wf_enable: 1 (WiFi enabled), 0 (WiFi disabled)</td>
                      </tr>
                      <tr>
                        <td>wf_ssid: SSID (max. 32 Characters)</td>
                      </tr>
                      <tr>
                        <td>wf_auth: 0 (no encryption), 1 (WEP), 2 (WPA-PSK), 3 (WPA2-PSK)</td>
                      </tr>
                      <tr>
                        <td>wf_key: Key max. 63 Characters (Allowed special characters: &amp;="`\)</td>
                      </tr>
                      <tr>
                        <td>wf_enc: Key type 0 (TKIP), 1 (AES)</td>
                      </tr>
                      <tr>
                        <td>wf_mode: 0 (infra), 1 (ad-hoc)</td>
                      </tr>
                      <tr>
                        <th>Example</th>
                        <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=getwirelessattr</code></td>
                      </tr>
                    </table>
                  </div>


                </div>

                </section>  <!-- Close Media Container -->

                <div class="clearfix" style="margin-bottom: 50px;"></div>

          </div>

