<body>
    <% include ../../_partials/template/_outdated.ejs %>

    <div id="wrapper">

        <header><% include ../../_partials/template/_header.ejs %></header>
        <!-- Navbar-->

            <div class="container mb">
                <h1>720p CGI Commands - Set Alarm Snap Quantity Attribute</h1>
                <hr>
            </div>

            <div class="clearfix"></div>

        <!-- #############################################  Body ############################################# -->

            <div class="container main">

              <section class="row" style="margin-top: 15px;">
                <!-- Media Container -->

                <div class="col-xs-12">

                  <h2>Alarm :: Set Alarm Snapshot Quantity Attribute</h2>
                  <h3>param.cgi?cmd=setalarmsnapattr</h3>

                  <div class="table-responsive">
                    <table class="table table-hover">
                      <tr>
                        <th>Function</th>
                        <td>Set Alarm Snapshot Quantity</td>
                      </tr>
                      <tr>
                        <th>Authority</th>
                        <td>admin</td>
                      </tr>
                      <tr>
                        <th>Syntax</th>
                        <td><code>/cgi-bin/hi3510/param.cgi[?cmd=setalarmsnapattr&amp;-snap_count=]</code></td>
                      </tr>
                      <tr>
                        <th rowspan="7">Parameter</th>
                        <td>snap_chn: Take Snapshot from Channel [11, 12, 13]</td>
                      </tr>
                      <tr>
                        <td>snap_name: Alarm Snapshot Name on SD Card - leave Empty for Timestamp</td>
                      </tr>
                      <tr>
                        <td>snap_timer_name: Snapshot Name for Photoseries - leave Empty for Timestamp</td>
                      </tr>
                      <tr>
                        <td>snap_count: Get Alarm Snapshot Quantity to SD Card [1-8]</td>
                      </tr>
                      <tr>
                        <td>snap_name_mode: [0-1]</td>
                      </tr>
                      <tr>
                        <td>email_snap_count: Get Alarm Snapshot Quantity by Email [1-8]</td>
                      </tr>
                      <tr>
                        <td>ftp_snap_count: Get Alarm Snapshot Quantity to FTP Server [1-8]</td>
                      </tr>
                      <tr>
                        <th>Return</th>
                        <td>Success, Error</td>
                      </tr>
                      <tr>
                        <th>Example</th>
                        <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=setalarmsnapattr&-snap_count=2</code></td>
                      </tr>
                    </table>
                  </div>

                </div>

                </section>  <!-- Close Media Container -->

                <div class="clearfix" style="margin-bottom: 50px;"></div>

          </div>
