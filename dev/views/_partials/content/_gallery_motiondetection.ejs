<section class="row centered grid">

    <!-- Take Images from gallery-motiondetection.json for Gallery -->

    <% if (mdPic.length > 0) { %>
        <% for (i=0; i<mdPic.length; i++) { %>
            <div class="col-6 col-lg-4 mb">
                <a href="/Motion_Detection/<%= mdUrl[i] %>"><img src="<%= mdPic[i] %>" alt="<%= mdPicAlt[i] %>" class="img-fluid"></a>
            </div>
        <% } %>
    <% } %>

</section>
