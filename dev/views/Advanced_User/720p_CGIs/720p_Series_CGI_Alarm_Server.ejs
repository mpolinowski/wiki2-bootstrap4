    <% include ../../_partials/template/_outdated.ejs %>

    <div id="wrapper">

        <header><% include ../../_partials/template/_header.ejs %></header>
        <!-- Navbar-->

        <div class="wrap">
            <div class="container main">

            <!-- ------------------------------------------------------------ Reboot ----------------------------------------------------- -->

            <section class="container mt mb"> <!-- Media Container -->
              <div class="row mb">
                <h2><a href="HD_Manual_WebUI_Alarm_FTP">The Alarm Server Overlay</a></h2>
                <h3>cmd=setmdalarm-aname=server2</h3>

                <div class="table-responsive">
                  <table class="table table-hover">
                    <tr>
                      <th>Function</th>
                      <td>Activate the 3rd Alarmserver to send HTTP GET Requests</td>
                    </tr>
                    <tr>
                      <th>Authority</th>
                      <td>admin</td>
                    </tr>
                    <tr>
                      <th>Parameter</th>
                      <td>switch [on, off]</td>
                    </tr>
                    <tr>
                      <th>Return</th>
                      <td>Success, Error</td>
                    </tr>
                    <tr>
                      <th>Remark</th>
                      <td>See <a href="#">Param.cgi?cmd=getalarmserverattr</a> and <a href="#">Param.cgi?cmd=setalarmserverattr</a>. Only the 3rd Alarmserver is availble  - Others might be used for other purposes and should not be reconfigured! You can check the other Alarmserver with the GET command above.</td>
                    </tr>
                    <tr>
                      <th>Example</th>
                      <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=setmdalarm&-aname=server2&-switch=on</code></td>
                    </tr>
                  </table>
                </div>

                </div>
                <!-- /Row -->
                  <div class="row mb">
                    <h2><a href="HD_Manual_WebUI_Alarm_FTP">The FTP Overlay</a></h2>
                    <h3>cmd=setalarmserverattr</h3>

                    <div class="table-responsive">
                      <table class="table table-hover">
                        <tr>
                          <th>Function</th>
                          <td>Configure the 3rd Alarmserver</td>
                        </tr>
                        <tr>
                          <th>Authority</th>
                          <td>admin</td>
                        </tr>
                        <tr>
                          <th>Parameter</th>
                          <td>as_server [IP or Domain] Adress of your server<br/>as_port [1-65536] Serverport<br/>as_path [String]<br/>as_query1 and as_query2 Add a attributes &amp; value to your URL<br/>as_queryattr1 and as_queryattr2 Attribute of your variable<br/>as_queryval1 and as_queryval2 Value of your variable<br/>as_auth [0, 1] Activate/Deactivate Authentification<br/>as_username and as_password User login</td>
                        </tr>
                        <tr>
                          <th>Remark</th>
                          <td>See <a href="#">Param.cgi?cmd=getalarmserverattr</a> and <a href="#">Param.cgi?cmd=setalarmserverattr</a></td>
                        </tr>
                        <tr>
                          <th>Example</th>
                          <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=setalarmserverattr&-as_index=3&-as_server=192.168.1.114&-as_port=8999&-as_path=%2FPath%2FTo%2FScripts-as_query1=1&-as_queryattr1=ir&-as_queryval1=on&-as_query2=1&-as_queryattr2=ptz&-as_queryval2=2&-as_auth=1&-as_username=admin&-as_password=admin</code></td>
                        </tr>
                      </table>
                    </div>

                  </div>
                  <!-- /Row -->

            </section>
             <!-- /Media Container -->

          </div>
