<body>
    <% include ../../_partials/template/_outdated.ejs %>

    <div id="wrapper">

        <header><% include ../../_partials/template/_header.ejs %></header>
        <!-- Navbar-->

            <div class="container mb">
                <h1>720p CGI Commands - Set Alarm Input Output Attribute</h1>
                <hr>
            </div>

            <div class="clearfix"></div>

        <!-- #############################################  Body ############################################# -->

            <div class="container main">

              <section class="row" style="margin-top: 15px;">
                <!-- Media Container -->

                <div class="col-xs-12">

                  <h2>Alarm :: Set Alarm Input / Output Attribute</h2>
                  <h3>param.cgi?cmd=setioattr</h3>

                  <div class="table-responsive">
                    <table class="table table-hover">
                      <tr>
                        <th>Function</th>
                        <td>Set Input/Output Alarm Parameter</td>
                      </tr>
                      <tr>
                        <th>Authority</th>
                        <td>admin</td>
                      </tr>
                      <tr>
                        <th>Syntax</th>
                        <td><code>/cgi-bin/hi3510/param.cgi[?cmd=setioattr &amp;-io_enable=&amp;-io_flag=]</code></td>
                      </tr>
                      <tr>
                        <th rowspan="2">Parameter</th>
                        <td>io_enable: Enable or Disable IO Alarm-in (1,0)</td>
                      </tr>
                      <tr>
                        <td>io_flag: IO Alarm mode 0 (close), 1 (open)</td>
                      </tr>
                      <tr>
                        <th>Return</th>
                        <td>Success, Error</td>
                      </tr>
                      <tr>
                        <th>Example</th>
                        <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=setioattr&-io_enable=0&-io_flag=1</code></td>
                      </tr>
                    </table>
                  </div>

                </div>

                </section>  <!-- Close Media Container -->

                <div class="clearfix" style="margin-bottom: 50px;"></div>

          </div>
