<% include ../../_partials/template/_head.ejs %>

<body>
    <% include ../../_partials/template/_outdated.ejs %>

    <div id="wrapper">

        <header><% include ../../_partials/template/_header.ejs %></header>
        <!-- Navbar-->

            <div class="container mb">
                <h1>720p CGI Commands - Search Wireless</h1>
                <hr>
            </div>

            <div class="clearfix"></div>

            <div class="container main">

              <!-- ######################################## Body ####################################### -->

              <section class="row" style="margin-top: 15px;">
                <!-- Media Container -->

                <div class="col-xs-12">

                  <h2>Network :: Search Wireless</h2>
                  <h3>param.cgi?cmd=searchwireless</h3>

                  <div class="table-responsive">
                    <table class="table table-hover">
                      <tr>
                        <th>Function</th>
                        <td>Search Wireless Access Point</td>
                      </tr>
                      <tr>
                        <th>Authority</th>
                        <td>admin</td>
                      </tr>
                      <tr>
                        <th>Syntax</th>
                        <td><code>/cgi-bin/hi3510/param.cgi[?cmd=searchwireless]</code></td>
                      </tr>
                      <tr>
                        <th rowspan="8">Return</th>
                        <td>waccess_points: Wireless Network Signals</td>
                      </tr>
                      <tr>
                        <td>wchannel: Channel</td>
                      </tr>
                      <tr>
                        <td>wrssi: Signal Strength</td>
                      </tr>
                      <tr>
                        <td>wessid: SSID</td>
                      </tr>
                      <tr>
                        <td>wenc: Encryption Mode</td>
                      </tr>
                      <tr>
                        <td>wauth: Authority Mode</td>
                      </tr>
                      <tr>
                        <td>wnet: Connection mode(router mode, point to point mode)</td>
                      </tr>
                      <tr>
                        <td>Note: If waccess_points >1 the CGI will return more values for each variable - Example:<br/>
                        var waccess_points="2";<br/>
                        var wchannel=new Array();<br/>
                        var wrssi=new Array();<br/>
                        var wessid=new Array();<br/>
                        var wenc=new Array();<br/>
                        var wauth=new Array();<br/>
                        var wnet=new Array();<br/>
                        wchannel[0]="6" ;<br/>
                        wrssi[0]="-82" ;<br/>
                        wessid[0]="My WiFi" ;<br/>
                        wenc[0]="AES" ;<br/>
                        wauth[0]="WPA(2)-PSK" ;<br/>
                        wnet[0]="Infra" ;<br/>
                        wchannel[1]="11" ;<br/>
                        wrssi[1]="-62" ;<br/>
                        wessid[1]="Linksys" ;<br/>
                        wenc[1]="TKIP" ;<br/>
                        wauth[1]="WPA(2)-PSK" ;<br/>
                        wnet[1]="Infra" ;<br/></td>
                      </tr>
                      <tr>
                        <th>Example</th>
                        <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=searchwireless</code></td>
                      </tr>
                    </table>
                  </div>


                </div>

                </section>  <!-- Close Media Container -->

                <div class="clearfix" style="margin-bottom: 50px;"></div>

          </div>

          <section class="webui mb"><% include ../../_partials/content/_webui.ejs %></section>
              <!-- /webui -->

          <section class="social"><% include ../../_partials/template/_social.ejs %></section>
              <!-- /social -->

    <!-- ############################################# Footer ############################################# -->

    <footer><% include ../../_partials/template/_footer.ejs %></footer>

    <% include ../../_partials/template/_jsdefaults.ejs %>

  </div>
  <!-- /Wrapper -->

</body>

</html>
