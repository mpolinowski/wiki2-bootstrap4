<% include ../../_partials/template/_head.ejs %>

<body>
    <% include ../../_partials/template/_outdated.ejs %>

    <div id="wrapper">

        <header><% include ../../_partials/template/_header.ejs %></header>
        <!-- Navbar-->

            <div class="container mb">
                <h1>720p CGI Commands - MJPEG Videostream</h1>
                <hr>
            </div>
            
            <div class="clearfix"></div>

        <!-- #############################################  Body ############################################# -->

            <div class="container main">

              <section class="row" style="margin-top: 15px;">
                <!-- Media Container -->

                <div class="col-xs-12">

                  <h2>System :: MJPEG Stream</h2>
                  <h3>mjpegstream.cgi</h3>

                  <div class="table-responsive">
                    <table class="table table-hover">
                      <tr>
                        <th>Function</th>
                        <td>Motion JPG Videostream</td>
                      </tr>
                      <tr>
                        <th>Authority</th>
                        <td>admin, user and guest</td>
                      </tr>
                      <tr>
                        <th>Syntax</th>
                        <td><code>/cgi-bin/hi3510/[mjpegstream.cgi?-chn=&amp;-usr=&amp;-pwd=]</code></td>
                      </tr>
                      <tr>
                        <th rowspan="3">Parameter</th>
                        <td>chn: First stream (11), second stream (12), third stream (13)</td>
                      </tr>
                      <tr>
                        <td>usr: User name</td>
                      </tr>
                      <tr>
                        <td>pwd: Password</td>
                      </tr>
                      <tr>
                        <th>Return</th>
                        <td>auto.jpg</td>
                      </tr>
                      <tr>
                        <th>Example</th>
                        <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/mjpegstream.cgi?-chn=12</code></td>
                      </tr>
                    </table>
                  </div>

                </div>

                </section>  <!-- Close Media Container -->

                <div class="clearfix" style="margin-bottom: 50px;"></div>

          </div>
          <!-- /Container -->

          <section class="webui mb"><% include ../../_partials/content/_webui.ejs %></section>
              <!-- /webui -->

          <section class="social"><% include ../../_partials/template/_social.ejs %></section>
              <!-- /social -->

    <!-- ############################################# Footer ############################################# -->

    <footer><% include ../../_partials/template/_footer.ejs %></footer>

    <% include ../../_partials/template/_jsdefaults.ejs %>

  </div>
  <!-- /Wrapper -->

</body>

</html>
