            <section class="container mt mb"> <!-- Media Container -->
              <div class="row mb">
                <h2><a href="/index.php/HD_Manual_WebUI_Recording_Video">The Video Overlay</a></h2>
                <h3>param.cgi?cmd=setplanrecattr</h3>

                <div class="table-responsive">
                  <table class="table table-hover">
                    <tr>
                      <th>Function</th>
                      <td>Set Planned Video Recording Attribute</td>
                    </tr>
                    <tr>
                      <th>Authority</th>
                      <td>admin</td>
                    </tr>
                    <tr>
                      <th rowspan="3">Parameter</th>
                      <td>planrec_enable: 1 (enable plan recording), 0 (disable plan recording)</td>
                    </tr>
                    <tr>
                      <td>planrec_time: Recoding File Length (15-900seconds)</td>
                    </tr>
                    <tr>
                      <td>planrec_chn: [11, 12, 13] <small>first - third Channel</small></td>
                    </tr>
                    <tr>
                      <th>Remarks</th>
                      <td>See <a href="/720p_Series_CGI_List/Complete_CGI_List/Get_Planned_Recording_Attribute">Param.cgi?cmd=getplanrecattr</a> and <a href="/720p_Series_CGI_List/Complete_CGI_List/Set_Planned_Recording_Attribute">Param.cgi?cmd=setplanrecattr</a></td>
                    </tr>
                    <tr>
                      <th>Example</th>
                      <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=getplanrecattr</code><br/><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=getscheduleex&-ename=snap</code><br/><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=setplanrecattr&-planrec_enable=1&-planrec_time=600&-planrec_chn=11</code></td>
                    </tr>
                  </table>
                </div>

              </div>
              <!-- /Row -->

              <div class="row mb">
                <h2><a href="/index.php/HD_Manual_WebUI_Recording_Video">The Video Overlay</a></h2>
                <h3>param.cgi?cmd=set_instar_admin&amp;-index=44</h3>

                <div class="table-responsive">
                  <table class="table table-hover">
                    <tr>
                      <th>Function</th>
                      <td>Set File Length for Manual SD Recordings</td>
                    </tr>
                    <tr>
                      <th>Authority</th>
                      <td>admin</td>
                    </tr>
                    <tr>
                      <th rowspan="2">Parameter</th>
                      <td>index: 44</td>
                    </tr>
                    <tr>
                      <td>value: none (unlimited) - 60 (in minutes)</td>
                    </tr>
                    <tr>
                      <th>Remarks</th>
                      <td>See <a href="/720p_Series_CGI_List/Complete_CGI_List/Get_Admin_Variables">Param.cgi?cmd=get_instar_admin</a> and <a href="/720p_Series_CGI_List/Complete_CGI_List/Set_Admin_Variables">Param.cgi?cmd=set_instar_admin</a></td>
                    </tr>
                    <tr>
                      <th>Example</th>
                      <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=get_instar_admin&-index=44</code><br/><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=set_instar_admin&-index=44&-value=60</code></td>
                    </tr>
                  </table>
                </div>

              </div>
