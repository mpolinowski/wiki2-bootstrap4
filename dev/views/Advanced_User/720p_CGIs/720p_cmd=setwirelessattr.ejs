<% include ../../_partials/template/_head.ejs %>

<body>
    <% include ../../_partials/template/_outdated.ejs %>

    <div id="wrapper">

        <header><% include ../../_partials/template/_header.ejs %></header>
        <!-- Navbar-->

            <div class="container mb">
                <h1>720p CGI Commands - Set Wireless Attribute</h1>
                <hr>
            </div>

            <div class="clearfix"></div>

        <!-- #############################################  Body ############################################# -->

            <div class="container main">

              <section class="row" style="margin-top: 15px;">
                <!-- Media Container -->

                <div class="col-xs-12">

                  <h2>Network :: Set Wireless Attribute</h2>
                  <h3>param.cgi?cmd=setwirelessattr</h3>

                  <div class="table-responsive">
                    <table class="table table-hover">
                      <tr>
                        <th>Function</th>
                        <td>Set Wifi Parameter</td>
                      </tr>
                      <tr>
                        <th>Authority</th>
                        <td>admin</td>
                      </tr>
                      <tr>
                        <th>Syntax</th>
                        <td><code>/cgi-bin/hi3510/param.cgi[?cmd=setwirelessattr&amp;-wf_enable=&amp;-wf_ssid=&amp;-wf_auth=&amp;-wf_key=&amp;-wf_enc=&amp;-wf_mode=]</code></td>
                      </tr>
                      <tr>
                        <th rowspan="6">Parameter</th>
                        <td>wf_enable: 1 (WiFi enabled), 0 (WiFi disabled)</td>
                      </tr>
                      <tr>
                        <td>wf_ssid: SSID (max. 32 Characters)</td>
                      </tr>
                      <tr>
                        <td>wf_auth: 0 (no encryption), 1 (WEP), 2 (WPA-PSK), 3 (WPA2-PSK)</td>
                      </tr>
                      <tr>
                        <td>wf_key: Key max. 63 Characters (Allowed special characters: &amp;="`\)</td>
                      </tr>
                      <tr>
                        <td>wf_enc: Key type 0 (TKIP), 1 (AES)</td>
                      </tr>
                      <tr>
                        <td>wf_mode: 0 (infra), 1 (ad-hoc)</td>
                      </tr>
                      <tr>
                        <th>Return</th>
                        <td>Success, Error</td>
                      </tr>
                      <tr>
                        <th>Example</th>
                        <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=setwirelessattr&-wf_ssid=linksys&-wf_enable=1&amp;-wf_auth=2&amp;-wf_key=1234567890&amp;-wf_enc=0&amp;-wf_mode=0</code></td>
                      </tr>
                    </table>
                  </div>

                </div>

                </section>  <!-- Close Media Container -->

                <div class="clearfix" style="margin-bottom: 50px;"></div>

          </div>
          <!-- /Container -->

          <section class="webui mb"><% include ../../_partials/content/_webui.ejs %></section>
              <!-- /webui -->

          <section class="social"><% include ../../_partials/template/_social.ejs %></section>
              <!-- /social -->

    <!-- ############################################# Footer ############################################# -->

    <footer><% include ../../_partials/template/_footer.ejs %></footer>

    <% include ../../_partials/template/_jsdefaults.ejs %>

  </div>
  <!-- /Wrapper -->

</body>

</html>
