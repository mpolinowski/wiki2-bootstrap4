<% include ../../_partials/template/_head.ejs %>

<body>
    <% include ../../_partials/template/_outdated.ejs %>

    <div id="wrapper">

        <header><% include ../../_partials/template/_header.ejs %></header>
        <!-- Navbar-->

            <div class="container mb">
                <h1>1080p CGI Commands - Set Video Encoder Attribute</h1>
                <hr>
            </div>

            <div class="clearfix"></div>

        <!-- #############################################  Body ############################################# -->

            <div class="container main">

              <section class="row" style="margin-top: 15px;">
                <!-- Media Container -->

                  <div class="col-xs-12">

                      <h2>Video &amp; Audio :: Set Video Encoder Attribute</h2>
                      <h3>param.cgi?cmd=setvencattr</h3>

                      <div class="table-responsive">
                        <table class="table table-hover">
                          <tr>
                            <th>Function</th>
                            <td>Config device video encode parameter</td>
                          </tr>
                          <tr>
                            <th>Authority</th>
                            <td>admin</td>
                          </tr>
                          <tr>
                            <th>Syntax</th>
                            <td><code>/cgi-bin/hi3510/param.cgi[?cmd=setvencattr&amp;-chn=&amp;-bps=&amp;-fps=&amp;-brmode=&amp;-imagegrade=&amp;-gop=]</code></td>
                          </tr>
                          <tr>
                            <th rowspan="6">Parameter</th>
                            <td>Stream:<br/>chn=11 1st Stream<br/>chn=12 2nd Stream<br/>chn=13 3rd Stream</td>
                          </tr>
                          <tr>
                            <td>Bitrate:<br/>1080P：Range [32 ~ 8192 kbps]<br/>720P：Range [32 ~ 4096 kbps]<br/>D1：Range [32 ~ 2048 kbps]<br/>CIF：Range [32 ~ 512 kbps]<br/>QCIF：Range [32 ~ 256 kbps]<br/>VGA：Range [32 ~ 2048 kbps]<br/>QVGA：Range [32 ~ 512 kbps]<br/>QQVGA：Range [32 ~ 256 kbps]<br/></td>
                          </tr>
                          <tr>
                            <td>Framerate:<br/>PAL：Range [1 ~ 25]<br/>NTSC：Range [1 ~ 30]</td>
                          </tr>
                          <tr>
                            <td>Keyframe Interval: gop Range [2 ~ 150]</td>
                          </tr>
                          <tr>
                            <td>Video Encode Control: 0: fixed bit rate (CBR) 1: variable bit rate (VBR)</td>
                          </tr>
                          <tr>
                            <td>Video Encode Quality: imagegrade Range [1 ~ 6]<br/><small>The smaller the value, the better the image quality</small></td>
                          </tr>
                          <tr>
                            <th>Return</th>
                            <td>Success / Failed</td>
                          </tr>
                          <tr>
                            <th rowspan="3">Examples</th>
                            <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=setvencattr&-chn=11&-bps=1536&-fps=25&-brmode=1&-imagegrade=1&-gop=50</code></td>
                          </tr>
                          <tr>
                            <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=setvencattr&-chn=12&-bps=1536&-fps=25&-brmode=1&-imagegrade=1&-gop=50</code></td>
                          </tr>
                          <tr>
                            <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=setvencattr&-chn=11&-bps=1536&-fps=25</code></td>
                          </tr>
                        </table>
                      </div>

                    </div>

                </section>  <!-- Close Media Container -->

                <div class="clearfix" style="margin-bottom: 50px;"></div>

          </div>
          <!-- /Container -->

          <section class="webui mb"><% include ../../_partials/content/_webui.ejs %></section>
              <!-- /webui -->

          <section class="social"><% include ../../_partials/template/_social.ejs %></section>
