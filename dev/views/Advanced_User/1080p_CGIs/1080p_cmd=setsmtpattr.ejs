<% include ../../_partials/template/_head.ejs %>

<body>
    <% include ../../_partials/template/_outdated.ejs %>

    <div id="wrapper">

        <header><% include ../../_partials/template/_header.ejs %></header>
        <!-- Navbar-->

            <div class="container mb">
                <h1>1080p CGI Commands - Set SMTP Attributes</h1>
                <hr>
            </div>

            <div class="clearfix"></div>

            <div class="container main">

              <!-- ######################################## Body ####################################### -->

              <section class="row" style="margin-top: 15px;">
                <!-- Media Container -->

                  <div class="col-xs-12">

                    <h2>SMTP :: Set SMTP Attributes</h2>
                    <h3>param.cgi?cmd=setsmtpattr</h3>

                    <div class="table-responsive">
                      <table class="table table-hover">
                        <tr>
                          <th>Function</th>
                          <td>Set SMTP Parameter</td>
                        </tr>
                        <tr>
                          <th>Authority</th>
                          <td>admin</td>
                        </tr>
                        <tr>
                          <th>Syntax</th>
                          <td><code>/cgi-bin/hi3510/param.cgi[?cmd=setsmtpattr&amp;-ma_server=&amp;-ma_port=&amp;-ma_ssl=&amp;-ma_logintype=&amp;-ma_username=&amp;-ma_password=&amp;-ma_from=&amp;-ma_to=&amp;-ma_subject=&amp;-ma_text=]</code></td>
                        </tr>
                        <tr>
                          <th rowspan="10">Parameter</th>
                          <td>ma_server: SMTP Server Address</td>
                        </tr>
                        <tr>
                          <td>ma_port: SMTP Server Port</td>
                        </tr>
                        <tr>
                          <td>ma_ssl: Enable (1) or Disable (0) SSL</td>
                        </tr>
                        <tr>
                          <td>ma_logintype: Enable (1) or Disable (3) Authentication</td>
                        </tr>
                        <tr>
                          <td>ma_username: SMTP Server Username</td>
                        </tr>
                        <tr>
                          <td>ma_password: SMTP Server Password</td>
                        </tr>
                        <tr>
                          <td>ma_from: Server Address</td>
                        </tr>
                        <tr>
                          <td>ma_to: Receiver Address</td>
                        </tr>
                        <tr>
                          <td>ma_subject: Mail Subject</td>
                        </tr>
                        <tr>
                          <td>ma_text: Mail Content</td>
                        </tr>
                        <tr>
                          <th>Return</th>
                          <td>Success, Error</td>
                        </tr>
                        <tr>
                          <th>Example</th>
                          <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=setsmtpattr&-ma_server=stmp.example.com&-ma_from=123@example.com&amp;-ma_to=456@example.com&amp;-ma_subject=demosubject&amp;-ma_text=demotext&amp;-ma_logintype=1&amp;-ma_username=demo&amp;-ma_password=123456&amp;-ma_port=25&amp;-ma_ssl=0</code></td>
                        </tr>
                      </table>
                    </div>

                  </div>

              </section>  <!-- Close Media Container -->

              <div class="clearfix" style="margin-bottom: 50px;"></div>

          </div>

