<% include ../../_partials/template/_head.ejs %>

<body>
    <% include ../../_partials/template/_outdated.ejs %>

    <div id="wrapper">

        <header><% include ../../_partials/template/_header.ejs %></header>
        <!-- Navbar-->

            <div class="container mb">
                <h1>1080p CGI Commands - Get Image Attribute</h1>
                <hr>
            </div>

            <div class="clearfix"></div>

            <div class="container main">

              <!-- ######################################## Body ####################################### -->

              <section class="row" style="margin-top: 15px;"> <!-- Media Container -->

                <div class="col-xs-12">

                  <h2>Video &amp; Audio :: Get Image Attribute</h2>
                  <h3>param.cgi?cmd=getimageattr</h3>

                  <div class="table-responsive">
                    <table class="table table-hover">
                      <tr>
                        <th>Function</th>
                        <td>Get image parameter</td>
                      </tr>
                      <tr>
                        <th>Authority</th>
                        <td>admin</td>
                      </tr>
                      <tr>
                        <th>Syntax</th>
                        <td><code>/cgi-bin/hi3510/param.cgi[?cmd=getimageattr]</code></td>
                      </tr>
                      <tr>
                        <th rowspan="11">Return</th>
                        <td>brightness</td>
                      </tr>
                      <tr>
                        <td>saturation</td>
                      </tr>
                      <tr>
                        <td>contrast</td>
                      </tr>
                      <tr>
                        <td>sharpness</td>
                      </tr>
                      <tr>
                        <td>hue</td>
                      </tr>
                      <tr>
                        <td>flip: on, off</td>
                      </tr>
                      <tr>
                        <td>mirror: on, off</td>
                      </tr>
                      <tr>
                        <td>night: on, off</td>
                      </tr>
                      <tr>
                        <td>shutter</td>
                      </tr>
                      <tr>
                        <td>wdr: on, off</td>
                      </tr>
                      <tr>
                        <td>gc: Night illumination, Gain Control</td>
                      </tr>
                      <tr>
                        <th>Remarks</th>
                        <td>Different device, different image parameter, for details, pleae refer to Appendix 1 (Device Type Definition)</td>
                      </tr>
                      <tr>
                        <th>Example</th>
                        <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=getimageattr</code></td>
                      </tr>
                    </table>
                  </div>

                </div>

                </section>  <!-- Close Media Container -->

                <div class="clearfix" style="margin-bottom: 50px;"></div>

          </div>

          <section class="webui mb"><% include ../../_partials/content/_webui.ejs %></section>
              <!-- /webui -->

          <section class="social"><% include ../../_partials/template/_social.ejs %></section>
