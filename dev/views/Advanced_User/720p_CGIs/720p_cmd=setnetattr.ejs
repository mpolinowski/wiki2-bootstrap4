<% include ../../_partials/template/_head.ejs %>

<body>
    <% include ../../_partials/template/_outdated.ejs %>

    <div id="wrapper">

        <header><% include ../../_partials/template/_header.ejs %></header>
        <!-- Navbar-->

            <div class="container mb">
                <h1>720p CGI Commands - Set Network Attribute</h1>
                <hr>
            </div>

            <div class="clearfix"></div>

        <!-- #############################################  Body ############################################# -->

            <div class="container main">

              <section class="row" style="margin-top: 15px;">
                <!-- Media Container -->

                <div class="col-xs-12">

                  <h2>Network :: Set Device Network Parameter</h2>
                  <h3>param.cgi?cmd=setnetattr</h3>

                  <div class="table-responsive">
                    <table class="table table-hover">
                      <tr>
                        <th>Function</th>
                        <td>Set LAN Network Parameter</td>
                      </tr>
                      <tr>
                        <th>Authority</th>
                        <td>admin</td>
                      </tr>
                      <tr>
                        <th>Syntax</th>
                        <td><code>/cgi-bin/hi3510/param.cgi[?cmd=setnetattr&amp;-dhcp=&amp;-ipaddr=&amp;-netmask=&amp;-gateway=&amp;-dnsstat=&amp;-fdnsip=&amp;-sdnsip=]</code></td>
                      </tr>
                      <tr>
                        <th rowspan="7">Parameter</th>
                        <td>dhcp: on (DHCP enabled), off: (DHCP disabled)</td>
                      </tr>
                      <tr>
                        <td>ipaddr: LAN IP Address</td>
                      </tr>
                      <tr>
                        <td>netmask: LAN Subnet Mask</td>
                      </tr>
                      <tr>
                        <td>gateway: LAN Gateway</td>
                      </tr>
                      <tr>
                        <td>dnsstat: DNS Status: 0 (manually), 1 (from DHCP Server)</td>
                      </tr>
                      <tr>
                        <td>fdnsip: Primary DNS</td>
                      </tr>
                      <tr>
                        <td>sdnsip: Secondary DNS</td>
                      </tr>
                      <tr>
                        <th>Return</th>
                        <td>Success, Error</td>
                      </tr>
                      <tr>
                        <th>Example</th>
                        <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=setnetattr&amp;-dhcp=off&amp;-ipaddr=192.168.1.88&amp;-netmask=255.255.255.0&amp;-gateway=192.168.1.1&amp;-dnsstat=0&amp;-fdnsip=192.168.1.1&amp;-sdnsip=</code></td>
                      </tr>
                    </table>
                  </div>

                </div>

                </section>  <!-- Close Media Container -->

                <div class="clearfix" style="margin-bottom: 50px;"></div>

          </div>
          <!-- /Container -->

          <section class="webui mb"><% include ../../_partials/content/_webui.ejs %></section>
              <!-- /webui -->

          <section class="social"><% include ../../_partials/template/_social.ejs %></section>
              <!-- /social -->

    <!-- ############################################# Footer ############################################# -->

    <footer><% include ../../_partials/template/_footer.ejs %></footer>

    <% include ../../_partials/template/_jsdefaults.ejs %>

  </div>
  <!-- /Wrapper -->

</body>

</html>
