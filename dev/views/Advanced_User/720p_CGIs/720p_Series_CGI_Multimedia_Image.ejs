            <section class="container mt mb"> <!-- Media Container -->
              <div class="row mb">
                <h2><a href="/index.php/HD_Manual_WebUI_Multimedia_Image">The Image Overlay</a></h2>
                <h3>param.cgi?cmd=setimageattr</h3>

                <div class="table-responsive">
                  <table class="table table-hover">
                    <tr>
                      <th>Function</th>
                      <td>Set Image Attributes</td>
                    </tr>
                    <tr>
                      <th>Authority</th>
                      <td>admin</td>
                    </tr>
                    <tr>
                      <th rowspan="12">Parameter</th>
                      <td>brightness: [0-255]</td>
                    </tr>
                    <tr>
                      <td>saturation: [0-255]</td>
                    </tr>
                    <tr>
                      <td>sharpness:</td>
                    </tr>
                    <tr>
                      <td>contrast: [0-7]</td>
                    </tr>
                    <tr>
                      <td>hue: [0-255]</td>
                    </tr>
                    <tr>
                      <td>night: Night Mode [on, off]</td>
                    </tr>
                    <tr>
                      <td>shutter:</td>
                    </tr>
                    <tr>
                      <td>flip: Flip the Image [on, off]</td>
                    </tr>
                    <tr>
                      <td>mirror: Mirror the Image [on, off]</td>
                    </tr>
                    <tr>
                      <td>gc: Night illumination, value Range [0 (auto) -255]</td>
                    </tr>
                    <tr>
                      <td>noise: Noise Reduction [on, off]</td>
                    </tr>
                    <tr>
                      <td>wdr: Wide Dynamic Range [on - off]</td>
                    </tr>
                    <tr>
                      <th>Remarks</th>
                      <td>See <a href="/index.php/HD_CGI_Commands_-_Get_Image_Attribute">Param.cgi?cmd=getimageattr</a> and <a href="/index.php/HD_CGI_Commands_-_Set_Image_Attribute">Param.cgi?cmd=setimageattr</a></td>
                    </tr>
                    <tr>
                      <th>Example</th>
                      <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510//param.cgi?cmd=getimageattr</code><br/><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510//param.cgi?cmd=setimageattr&-brightness=4&-saturation=3&-contrast=4&-flip=off&-mirror=off&-shutter=0&-sharpness=4</code></td>
                    </tr>
                  </table>
                </div>

            </div>
            <!-- /Row -->

            <div class="row mb">
              <h2><a href="/index.php/HD_Kamera_CGIs_Multimedia_Image">The Image Overlay</a></h2>
              <h3>param.cgi?cmd=setoverlayattr</h3>

              <div class="table-responsive">
                <table class="table table-hover">
                  <tr>
                    <th>Function</th>
                    <td>Set the Video Attributes</td>
                  </tr>
                  <tr>
                    <th>Authority</th>
                    <td>admin</td>
                  </tr>
                  <tr>
                    <th rowspan="3">Parameter</th>
                    <td>region: 0 (OSD Timestamp), 1 (Camera Name)</td>
                  </tr>
                  <tr>
                    <td>show: 1 (show), 0 (hide)</td>
                  </tr>
                  <tr>
                    <td>name: OSD Name Only English, Max. 16 Characters</td>
                  </tr>
                  <tr>
                    <th>Remarks</th>
                    <td>See <a href="/index.php/HD_CGI_Commands_-_Get_Overlay_Attribute">Param.cgi?cmd=getoverlayattr</a> and <a href="/index.php/HD_CGI_Commands_-_Set_Overlay_Attribute">Param.cgi?cmd=setoverlayattr</a></td>
                  </tr>
                  <tr>
                    <th>Example</th>
                    <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=getimageattr&cmd=getoverlayattr&-region=0&cmd=getoverlayattr&-region=1</code><br/><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=setoverlayattr&-region=1&-show=1&-name=MyCamera&cmd=setoverlayattr&-region=0&-show=1</code></td>
                  </tr>
                </table>
              </div>

            </div>
