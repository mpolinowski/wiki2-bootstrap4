    <% include ../../_partials/template/_outdated.ejs %>

    <div id="wrapper">

        <header><% include ../../_partials/template/_header.ejs %></header>
        <!-- Navbar-->

            <div class="container mb">
                <h1>720p CGI Commands - Get Alarm And Planned Recording Schedule</h1>
                <hr>
            </div>

            <div class="clearfix"></div>

            <div class="container main">

              <!-- ######################################## Body ####################################### -->

              <section class="row" style="margin-top: 15px;">
                <!-- Media Container -->

                <div class="col-xs-12">

                  <h2>Alarm :: Get Alarm And Planned Recording Schedule</h2>
                  <h3>param.cgi?cmd=getscheduleex</h3>

                  <div class="table-responsive">
                    <table class="table table-hover">
                      <tr>
                        <th>Function</th>
                        <td>Get Alarm and Planned Recording Time Window</td>
                      </tr>
                      <tr>
                        <th>Authority</th>
                        <td>admin</td>
                      </tr>
                      <tr>
                        <th>Syntax</th>
                        <td><code>/cgi-bin/hi3510/param.cgi[?cmd=setscheduleex&amp;-ename=]</code></td>
                      </tr>
                      <tr>
                        <th>Parameter</th>
                        <td>ename: md (alarm armed), plan (planned recording)</td>
                      </tr>
                      <tr>
                        <th>Return</th>
                        <td>week0：Sun, week1：Mon, week2：Tue, week3：Wed, week4：Thu, week5：Fri, week6：Sat, P : armed, N : disabled<br/>week0=PPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPP<br/>week1=PPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPP<br/>week2=PPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPP<br/>week3=PPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPP<br/>week4=PPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPP<br/>week5=PPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPP<br/>week6=PPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPPP<br/></td>
                      </tr>
                      <tr>
                        <th>Remarks</th>
                        <td>Parameter-week0 ~ -week6, every day is divided by 48 creating 30min time segments. Use P or N to indicate if the recording is armed (P) or closed (N). Time starts at 0:00.</td>
                      </tr>
                      <tr>
                        <th>Example</th>
                        <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=getscheduleex&-ename=md</code></td>
                      </tr>
                    </table>
                  </div>

                </div>

                </section>  <!-- Close Media Container -->

                <div class="clearfix" style="margin-bottom: 50px;"></div>

          </div>

