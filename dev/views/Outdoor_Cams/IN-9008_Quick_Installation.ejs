                      <div class="card-container">
                          <div class="onecard">
                              <div class="title">
                                  <h3><span class="strong">I-a</span>   Open the Camera Body</h3>
                              </div>
                              <div class="image">
                                  <amp-img
                                      on="tap:lightbox1"
                                      role="button"
                                      tabindex="0"
                                      src="/images/IN-9008-Quick-Installation-I-A.png" width="842" height="595" layout="responsive">
