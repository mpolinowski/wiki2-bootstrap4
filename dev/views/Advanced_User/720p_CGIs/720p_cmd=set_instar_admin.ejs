<body>
    <% include ../../_partials/template/_outdated.ejs %>

    <div id="wrapper">

        <header><% include ../../_partials/template/_header.ejs %></header>
        <!-- Navbar-->

            <div class="container mb">
                <h1>720p CGI Commands - Set Admin Variables</h1>
                <hr>
            </div>

            <div class="clearfix"></div>

        <!-- #############################################  Body ############################################# -->

            <div class="container main">

              <section class="row" style="margin-top: 15px;">
                <!-- Media Container -->

                <div class="col-xs-12">

                  <h2>System :: Set Admin Variables</h2>
                  <h3>param.cgi?cmd=set_instar_admin</h3>

                  <div class="table-responsive">
                    <table class="table table-hover">
                      <tr>
                        <th>Function</th>
                        <td>Set Admin Variables</td>
                      </tr>
                      <tr>
                        <th>Authority</th>
                        <td>admin</td>
                      </tr>
                      <tr>
                        <th>Syntax</th>
                        <td><code>/cgi-bin/hi3510/param.cgi[?cmd=set_instar_admin&amp;-index=&amp;-value=]</code></td>
                      </tr>
                      <tr>
                        <th>Parameter</th>
                        <td>
                          admin_value[0] : multi camera view data for camera 0<br/>
                          admin_value[1] : multi camera view data for camera 1<br/>
                          admin_value[2] : multi camera view data for camera 2<br/>
                          admin_value[3] : multi camera view data for camera 3<br/>
                          admin_value[4] : multi camera view data for camera 4<br/>
                          admin_value[5] : multi camera view data for camera 5<br/>
                          admin_value[6] : multi camera view data for camera 6<br/>
                          admin_value[7] : multi camera view data for camera 7<br/>
                          admin_value[8] : multi camera view data for camera 8<br/>
                          admin_value[9] : hide camera data sheet ; if admin_value[9] == "1" hide menu else show it<br/>
                          admin_value[10] : webUI language ; 1 = german, 2 = english, 3 = french, 4 = chinese<br/>
                          admin_value[11] : help status<br/>
                          admin_value[12] : installation wizard; if 0 the wizard is loaded otherwise ignored<br/>
                          admin_value[13] : <br/>
                          admin_value[14] : 1 ; sets Area Selection to area.html<br/>
                          admin_value[15] : set status of relay<br/>
                          admin_value[16] : <br/>
                          admin_value[17] : <br/>
                          admin_value[18] : <br/>
                          admin_value[19] : <br/>
                          admin_value[20] : <br/>
                          admin_value[21] : <br/>
                          admin_value[22] : <br/>
                          admin_value[23] : <br/>
                          admin_value[24] : <br/>
                          admin_value[25] : <br/>
                          admin_value[26] : <br/>
                          admin_value[27] : <br/>
                          admin_value[28] : <br/>
                          admin_value[29] : <br/>
                          admin_value[30] : <br/>
                          admin_value[31] : <br/>
                          admin_value[32] : h264 resolution [11, 12, 13], mjpeg resolution [11, 12, 13], mode [h.264 (0), MJPEG (1)] mjpeg fps [e.g. 3] - default 11:11:0:3<br/>
                          admin_value[33] : <br/>
                          admin_value[34] : <br/>
                          admin_value[35] : INSTAR JPEG MODE [true, false]<br/>
                          admin_value[36] : <br/>
                          admin_value[37] : <br/>
                          admin_value[38] : <br/>
                          admin_value[39] : <br/>
                          admin_value[40] : <br/>
                          admin_value[41] : <br/>
                          admin_value[42] : video plugin (flash, activeX, quicktime)<br/>
                          admin_value[43] : Length of manual SD record<br/>
                          admin_value[44] : INSTAR JPEG MODE; Timeout determines the time the JPEG mode waits for the to load a new image (default 250ms)<br/>
                          admin_value[45] : PTZ Steps (0 = off; 1 = on)<br/>
                          admin_value[46] : <br/>
                          admin_value[47] : <br/>
                          admin_value[48] : INSTAR show QTConfirm --> if this is used it is wrong //init/null pushserver init status [true, false]<br/>
                          admin_value[49] : holds the status of day light saving time [on, off]<br/>
                        </td>
                      </tr>
                      <tr>
                        <th>Return</th>
                        <td>Success, Error</td>
                      </tr>
                      <tr>
                        <th>Example</th>
                        <td><code>http://admin:instar@192.168.178.88/param.cgi?cmd=set_instar_admin&-index=11&-value=2&cmd=set_instar_admin&-index=50&-value=off</code></td>
                      </tr>
                    </table>
                  </div>

                </div>

                </section>  <!-- Close Media Container -->

                <div class="clearfix" style="margin-bottom: 50px;"></div>

          </div>
          <!-- /Container -->

          <section class="webui mb"><% include ../../_partials/content/_webui.ejs %></section>
              <!-- /webui -->

          <section class="social"><% include ../../_partials/template/_social.ejs %></section>
