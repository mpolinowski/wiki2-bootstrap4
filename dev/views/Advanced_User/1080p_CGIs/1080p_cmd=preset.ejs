<% include ../../_partials/template/_head.ejs %>

<body>
    <% include ../../_partials/template/_outdated.ejs %>

    <div id="wrapper">

        <header><% include ../../_partials/template/_header.ejs %></header>
        <!-- Navbar-->

            <div class="container mb">
                <h1>1080p CGI Commands - PTZ Preset</h1>
                <hr>
            </div>

            <div class="clearfix"></div>

            <div class="container main">

              <!-- ######################################## Body ####################################### -->

              <section class="row" style="margin-top: 15px;">
                <!-- Media Container -->

                <div class="col-xs-12">

                  <h2>PTZ Control :: PTZ Preset</h2>
                  <h3>param.cgi?cmd=preset</h3>

                  <div class="table-responsive">
                    <table class="table table-hover">
                      <tr>
                        <th>Function</th>
                        <td>PTZ Preset</td>
                      </tr>
                      <tr>
                        <th>Authority</th>
                        <td>admin, user</td>
                      </tr>
                      <tr>
                        <th>Syntax</th>
                        <td><code>/cgi-bin/hi3510/param.cgi?cmd=preset[&amp;-act=&amp;-status=&amp;-number=]</code></td>
                      </tr>
                      <tr>
                        <th rowspan="3">Parameter</th>
                        <td>act: goto (call preset), set (set preset)</td>
                      </tr>
                      <tr>
                        <td>status: 0 (delete preset), 1 (save preset)</td>
                      </tr>
                      <tr>
                        <td>number: preset number (1-8)</td>
                      </tr>
                      <tr>
                        <th>Return</th>
                        <td>Success, Error</td>
                      </tr>
                      <tr>
                        <th>Remarks</th>
                        <td>Different device PTZ Parameter is different , please refer Appendix I</td>
                      </tr>
                      <tr>
                        <th rowspan="2">Example</th>
                        <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=preset&-act=goto&-number=1</code></td>
                      </tr>
                      <tr>
                        <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=preset&-act=set&-status=1&-number=1</code></td>
                      </tr>
                    </table>
                  </div>


                </div>

                </section>  <!-- Close Media Container -->

                <div class="clearfix" style="margin-bottom: 50px;"></div>

          </div>

          <section class="webui mb"><% include ../../_partials/content/_webui.ejs %></section>
              <!-- /webui -->

          <section class="social"><% include ../../_partials/template/_social.ejs %></section>
              <!-- /social -->

    <!-- ############################################# Footer ############################################# -->

    <footer><% include ../../_partials/template/_footer.ejs %></footer>

    <% include ../../_partials/template/_jsdefaults.ejs %>

  </div>
  <!-- /Wrapper -->

</body>

</html>
