<% include ../../_partials/template/_head.ejs %>

<body>
    <% include ../../_partials/template/_outdated.ejs %>

    <div id="wrapper">

        <header><% include ../../_partials/template/_header.ejs %></header>
        <!-- Navbar-->

        <div class="wrap">
            <div class="container main">

            <!-- ------------------------------------------------------------ Reboot ----------------------------------------------------- -->

            <section class="container mt mb"> <!-- Media Container -->
              <div class="row mb">
                <h2><a href="HD_Manual_WebUI_Alarm_FTP">The FTP Overlay</a></h2>
                <h3>param.cgi?cmd=setftpattr</h3>

                <div class="table-responsive">
                  <table class="table table-hover">
                    <tr>
                      <th>Function</th>
                      <td>Set FTP Server Parameter</td>
                    </tr>
                    <tr>
                      <th>Authority</th>
                      <td>admin</td>
                    </tr>
                    <tr>
                      <th>Parameter</th>
                      <td>ft_server: FTP Server Address<br/>ft_port：FTP Server Port<br/>ft_username: FTP Username<br/>ft_password：FTP Password<br/>ft_mode：FTP Mode Port (0) or Passive (1)<br/>ft_dirname：FTP Directory<br/>ft_dirmode: 0 (Create a new Folder every Day), 1 (All Files in One Directory)</td>
                    </tr>
                    <tr>
                      <th>Return</th>
                      <td>Success, Error</td>
                    </tr>
                    <tr>
                      <th>Remark</th>
                      <td>See <a href="/720p_Series_CGI_List/Complete_CGI_List/Get_FTP_Attributes">Param.cgi?cmd=getftpattr</a> and <a href="/720p_Series_CGI_List/Complete_CGI_List/Set_FTP_Attributes">Param.cgi?cmd=setftpattr</a></td>
                    </tr>
                    <tr>
                      <th>Example</th>
                      <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=getftpattr</code><br/><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=setftpattr&-ft_server=192.168.178.1&-ft_port=21&-ft_username=ftpuser&-ft_password=password&-ft_mode=1&-ft_dirname=./camera&-ft_autocreatedir=1&-ft_dirmode=0</code></td>
                    </tr>
                  </table>
                </div>

                </div>
                <!-- /Row -->
                  <div class="row mb">
                    <h2><a href="HD_Manual_WebUI_Alarm_FTP">The FTP Overlay</a></h2>
                    <h3>param.cgi?cmd=testftp</h3>

                    <div class="table-responsive">
                      <table class="table table-hover">
                        <tr>
                          <th>Function</th>
                          <td>Test FTP Parameter</td>
                        </tr>
                        <tr>
                          <th>Authority</th>
                          <td>admin</td>
                        </tr>
                        <tr>
                          <th>Return</th>
                          <td>0 (successful)<br/>-1 (unknown error)<br/>-2 (no connection to server)<br/>-3 (username or password error)<br/>-4 (path error)</td>
                        </tr>
                        <tr>
                          <th>Remark</th>
                          <td>See <a href="/720p_Series_CGI_List/Complete_CGI_List/Test_FTP">Param.cgi?cmd=testftp</a></td>
                        </tr>
                        <tr>
                          <th>Example</th>
                          <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=testsmtp</code></td>
                        </tr>
                      </table>
                    </div>

                  </div>
                  <!-- /Row -->

            </section>
             <!-- /Media Container -->

          </div>
          <!-- /Container -->
        </div>
        <!-- /Wrap -->

          <section class="webui mb"><% include ../../_partials/content/_webui.ejs %></section>
