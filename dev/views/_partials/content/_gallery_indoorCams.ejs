<section class="row mt grid">

    <!-- Take Images from gallery-quickinstallation.json for Gallery -->

    <% if (indoorPic.length > 0) { %>
        <% for (i=indoorPic.length -3; i>=0; i--) { %>
            <div class="col-6 col-lg-4 mb">
                <a href="/<%= indoorUrl[i] %>"><img src="<%= indoorPic[i] %>" alt="<%= indoorPicAlt[i] %>" class="img-fluid"></a>
            </div>
        <% } %>
    <% } %>

</section>
